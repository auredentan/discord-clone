    "framer-motion": "^4",
    "next": "^10.2.0",
    "next-offline": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "xstate": "^4.19.1"
  },
  "devDependencies": {
